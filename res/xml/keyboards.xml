<?xml version="1.0" encoding="UTF-8"?>
<Keyboards>
	<!--
		id := a UUID (you may want to generate this using our simple Java
		program at: /trunk/project/dict_creation (uuidgen_Windows.bat or
		uuidgen_Linux) nameResId := the resource Id which holds the name of
		the keyboard. See strings.xml. layoutResId := the resource Id which
		holds the layout definition of this keyboard. The sample is an English
		QWERTY keyboard. defaultDictionaryLocale := a locate identifier for
		this keyboard. E.g., English will be en, Hebrew will be iw, etc.
		description := some text you would like to add. index := the sort
		index of this keyboard in the Settings page.
	-->
	<Keyboard nameResId="@string/sk_keyboard" iconResId="@drawable/sk"
		layoutResId="@xml/sk_qwerty" landscapeResId="@xml/sk_qwerty"
		id="8c9bcbd0-3cda-4bee-bbab-198173a8afa1" description="Multitap for accent letters"
		index="1" physicalKeyboardMappingResId="@xml/sk_qwerty_physical"
		defaultDictionaryLocale="sk" />

	<Keyboard nameResId="@string/sk_keyboard_nomtap" iconResId="@drawable/sk"
		layoutResId="@xml/sk_qwerty_nomtap" landscapeResId="@xml/sk_qwerty_nomtap"
		id="1f9d583b-dfe3-4892-a4fc-cc1f0835838d" description="No multitap for accent letters"
		index="2" physicalKeyboardMappingResId="@xml/sk_qwerty_physical"
		defaultDictionaryLocale="sk" />
	
	<Keyboard nameResId="@string/sk_keyboard_qwertz" iconResId="@drawable/sk"
		layoutResId="@xml/sk_qwertz" landscapeResId="@xml/sk_qwertz"
		id="47bf7b6a-db70-4d8f-ad20-cb338b135250" description="Multitap qwertz for accent letters"
		index="3" physicalKeyboardMappingResId="@xml/sk_qwertz_physical"
		defaultDictionaryLocale="sk" />

	<Keyboard nameResId="@string/sk_keyboard_qwertz_nomtap" iconResId="@drawable/sk"
		layoutResId="@xml/sk_qwertz_nomtap" landscapeResId="@xml/sk_qwertz_nomtap"
		id="eb6e45a0-7b3e-4094-a6d5-3f381cacfeb4" description="No multitap qwertz for accent letters"
		index="4" physicalKeyboardMappingResId="@xml/sk_qwertz_physical"
		defaultDictionaryLocale="sk" />
	
	
</Keyboards>